defines = -DV8_DEPRECATION_WARNINGS -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DDONT_EMBED_BUILD_METADATA $
    -DCLD_VERSION=2 -DDISABLE_FTP_SUPPORT=1 -DWEBRTC_RESTRICT_LOGGING $
    -DEXPAT_RELATIVE_PATH -DWEBRTC_MODULE_UTILITY_VIDEO -DWEBRTC_ARCH_ARM $
    -DWEBRTC_ARCH_ARM64_NEON -DWEBRTC_POSIX -DWEBRTC_MAC -DWEBRTC_IOS $
    -DUSE_LIBPCI=1 -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DNDEBUG $
    -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0 -DNS_BLOCK_ASSERTIONS=1 $
    -D_FORTIFY_SOURCE=2
includes = -Igen -I../../../.. $
    -I../../../../webrtc/modules/remote_bitrate_estimator
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS8.1.sdk $
    -Os -gdwarf-2 -fvisibility=hidden -Werror -Wnewline-eof $
    -miphoneos-version-min=7.0 -arch arm64 -Wall -Wendif-labels -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-selector-type-mismatch -Wno-unknown-warning-option $
    -Wheader-hygiene -Wno-char-subscripts $
    -Wno-unneeded-internal-declaration -Wno-covered-switch-default $
    -Wstring-conversion -Wno-c++11-narrowing -Wno-deprecated-register $
    -Wno-inconsistent-missing-override
cflags_c = -std=c99 -fcolor-diagnostics
cflags_cc = -std=c++11 -stdlib=libc++ -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -fcolor-diagnostics
cflags_objc = $cflags_c -Wobjc-missing-property-synthesis
cflags_objcc = $cflags_cc -Wobjc-missing-property-synthesis $
    -fobjc-call-cxx-cdtors

build $
    obj/webrtc/modules/remote_bitrate_estimator/test/rbe_components.bwe_test_logging.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/test/bwe_test_logging.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.aimd_rate_control.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/aimd_rate_control.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.inter_arrival.o: $
    cxx ../../../../webrtc/modules/remote_bitrate_estimator/inter_arrival.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.mimd_rate_control.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/mimd_rate_control.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.overuse_detector.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/overuse_detector.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.overuse_estimator.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/overuse_estimator.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_bitrate_estimator_abs_send_time.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/remote_bitrate_estimator_abs_send_time.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_bitrate_estimator_single_stream.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/remote_bitrate_estimator_single_stream.cc
build $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_rate_control.o: $
    cxx $
    ../../../../webrtc/modules/remote_bitrate_estimator/remote_rate_control.cc

build librbe_components.a: alink $
    obj/webrtc/modules/remote_bitrate_estimator/test/rbe_components.bwe_test_logging.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.aimd_rate_control.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.inter_arrival.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.mimd_rate_control.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.overuse_detector.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.overuse_estimator.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_bitrate_estimator_abs_send_time.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_bitrate_estimator_single_stream.o $
    obj/webrtc/modules/remote_bitrate_estimator/rbe_components.remote_rate_control.o
