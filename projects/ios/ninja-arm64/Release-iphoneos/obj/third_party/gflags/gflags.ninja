defines = -DV8_DEPRECATION_WARNINGS -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DDONT_EMBED_BUILD_METADATA $
    -DCLD_VERSION=2 -DDISABLE_FTP_SUPPORT=1 -DGFLAGS_DLL_DECL= $
    -DGFLAGS_DLL_DECLARE_FLAG= -DGFLAGS_DLL_DEFINE_FLAG= -DUSE_LIBPCI=1 $
    -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0 $
    -DNS_BLOCK_ASSERTIONS=1
includes = -Igen -I../../../../third_party/gflags/gen/posix/include/private $
    -I../../../../third_party/gflags/gen/posix/include $
    -I../../../../third_party/gflags/src
cflags = -isysroot $
    /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS8.1.sdk $
    -Os -gdwarf-2 -fvisibility=hidden -Wnewline-eof $
    -miphoneos-version-min=7.0 -arch arm64 -Wendif-labels $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-selector-type-mismatch -Wno-unknown-warning-option $
    -Wno-char-subscripts -Wno-unneeded-internal-declaration $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-deprecated-register -Wno-inconsistent-missing-override
cflags_c = -std=c99 -fcolor-diagnostics
cflags_cc = -std=c++11 -stdlib=libc++ -fno-rtti -fno-exceptions $
    -fvisibility-inlines-hidden -fno-threadsafe-statics -fcolor-diagnostics
cflags_objc = $cflags_c
cflags_objcc = $cflags_cc -fobjc-call-cxx-cdtors

build obj/third_party/gflags/src/gflags.gflags.o: cxx $
    ../../../../third_party/gflags/src/gflags.cc
build obj/third_party/gflags/src/gflags.gflags_completions.o: cxx $
    ../../../../third_party/gflags/src/gflags_completions.cc
build obj/third_party/gflags/src/gflags.gflags_reporting.o: cxx $
    ../../../../third_party/gflags/src/gflags_reporting.cc

build libgflags.a: alink obj/third_party/gflags/src/gflags.gflags.o $
    obj/third_party/gflags/src/gflags.gflags_completions.o $
    obj/third_party/gflags/src/gflags.gflags_reporting.o
