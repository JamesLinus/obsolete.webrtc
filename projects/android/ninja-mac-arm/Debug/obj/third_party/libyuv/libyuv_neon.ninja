defines = -DV8_DEPRECATION_WARNINGS -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC $
    -DDISABLE_NACL -DCHROMIUM_BUILD -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 $
    -DUSE_PROPRIETARY_CODECS -DENABLE_BROWSER_CDMS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_NOTIFICATIONS $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DDONT_EMBED_BUILD_METADATA $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_BASIC_PRINTING=1 -DENABLE_SUPERVISED_USERS=1 -DVIDEO_HOLE=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DUSE_LIBPCI=1 -DUSE_OPENSSL=1 $
    -DUSE_OPENSSL_CERTS=1 -DANDROID -D__GNU_SOURCE=1 -DUSE_STLPORT=1 $
    -D_STLP_USE_PTR_SPECIALIZATIONS=1 '-DCHROME_BUILD_ID=""' $
    -DHAVE_SYS_UIO_H -DDYNAMIC_ANNOTATIONS_ENABLED=1 $
    -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -Igen -I../../../../third_party/libyuv/include $
    -I../../../../third_party/libyuv
cflags = -fstack-protector --param=ssp-buffer-size=4 -fno-strict-aliasing $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -fvisibility=hidden -pipe -fPIC -Wno-unused-local-typedefs $
    -Wa,-mfpu=neon -Wno-format -march=armv7-a -mtune=generic-armv7-a $
    -mfloat-abi=softfp -mthumb -fno-tree-sra -fno-caller-saves -Wno-psabi $
    -mthumb-interwork -ffunction-sections -funwind-tables -g $
    -fstack-protector -fno-short-enums -finline-limit=64 -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -isystem$android_ndk_path/sources/cxx-stl/stlport/stlport $
    -Os -g -gdwarf-4 -fdata-sections -ffunction-sections $
    -fomit-frame-pointer -funwind-tables
cflags_c = 
cflags_cc = -fno-exceptions -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wno-deprecated -Wno-abi -std=gnu++11 $
    -Wno-narrowing -Wno-literal-suffix

build obj/third_party/libyuv/source/libyuv_neon.compare_neon.o: cxx $
    ../../../../third_party/libyuv/source/compare_neon.cc
build obj/third_party/libyuv/source/libyuv_neon.compare_neon64.o: cxx $
    ../../../../third_party/libyuv/source/compare_neon64.cc
build obj/third_party/libyuv/source/libyuv_neon.rotate_neon.o: cxx $
    ../../../../third_party/libyuv/source/rotate_neon.cc
build obj/third_party/libyuv/source/libyuv_neon.rotate_neon64.o: cxx $
    ../../../../third_party/libyuv/source/rotate_neon64.cc
build obj/third_party/libyuv/source/libyuv_neon.row_neon.o: cxx $
    ../../../../third_party/libyuv/source/row_neon.cc
build obj/third_party/libyuv/source/libyuv_neon.row_neon64.o: cxx $
    ../../../../third_party/libyuv/source/row_neon64.cc
build obj/third_party/libyuv/source/libyuv_neon.scale_neon.o: cxx $
    ../../../../third_party/libyuv/source/scale_neon.cc
build obj/third_party/libyuv/source/libyuv_neon.scale_neon64.o: cxx $
    ../../../../third_party/libyuv/source/scale_neon64.cc

build libyuv_neon.a: alink $
    obj/third_party/libyuv/source/libyuv_neon.compare_neon.o $
    obj/third_party/libyuv/source/libyuv_neon.compare_neon64.o $
    obj/third_party/libyuv/source/libyuv_neon.rotate_neon.o $
    obj/third_party/libyuv/source/libyuv_neon.rotate_neon64.o $
    obj/third_party/libyuv/source/libyuv_neon.row_neon.o $
    obj/third_party/libyuv/source/libyuv_neon.row_neon64.o $
    obj/third_party/libyuv/source/libyuv_neon.scale_neon.o $
    obj/third_party/libyuv/source/libyuv_neon.scale_neon64.o
