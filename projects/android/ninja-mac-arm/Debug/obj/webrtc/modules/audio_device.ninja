defines = -DV8_DEPRECATION_WARNINGS -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC $
    -DDISABLE_NACL -DCHROMIUM_BUILD -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 $
    -DUSE_PROPRIETARY_CODECS -DENABLE_BROWSER_CDMS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_NOTIFICATIONS $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DDONT_EMBED_BUILD_METADATA $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_BASIC_PRINTING=1 -DENABLE_SUPERVISED_USERS=1 -DVIDEO_HOLE=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DWEBRTC_RESTRICT_LOGGING $
    -DEXPAT_RELATIVE_PATH -DWEBRTC_MODULE_UTILITY_VIDEO -DWEBRTC_ARCH_ARM $
    -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON -DWEBRTC_POSIX $
    -DWEBRTC_LINUX -DWEBRTC_ANDROID -DWEBRTC_ANDROID_OPENSLES $
    -DUSE_LIBPCI=1 -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DANDROID $
    -D__GNU_SOURCE=1 -D_STLP_USE_PTR_SPECIALIZATIONS=1 $
    '-DCHROME_BUILD_ID=""' -DHAVE_SYS_UIO_H -DDYNAMIC_ANNOTATIONS_ENABLED=1 $
    -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -Igen -I../../../.. -I../../../../webrtc/modules/audio_device $
    -I../../../../webrtc/modules/interface $
    -I../../../../webrtc/modules/audio_device/include $
    -I../../../../webrtc/modules/audio_device/dummy $
    -I../../../../webrtc/modules/audio_device/android $
    -I../../../../webrtc/common_audio/resampler/include $
    -I../../../../webrtc/common_audio/signal_processing/include $
    -I../../../../webrtc/common_audio/vad/include $
    -I../../../../webrtc/system_wrappers/interface
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-unused-local-typedefs -Wextra -Wno-unused-parameter $
    -Wno-missing-field-initializers -Wno-strict-overflow -fno-builtin-cos $
    -fno-builtin-sin -fno-builtin-cosf -fno-builtin-sinf -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-strict-overflow -fno-builtin-cos -fno-builtin-sin $
    -fno-builtin-cosf -fno-builtin-sinf -march=armv7-a $
    -mtune=generic-armv7-a -mfpu=vfpv3-d16 -mfloat-abi=softfp -mthumb $
    -fno-tree-sra -fno-caller-saves -Wno-psabi -mthumb-interwork $
    -ffunction-sections -funwind-tables -g -fstack-protector $
    -fno-short-enums -finline-limit=64 -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -isystem$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.7/include $
    -isystem$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.7/libs/armeabi-v7a/include $
    -Os -g -gdwarf-4 -fdata-sections -ffunction-sections $
    -fomit-frame-pointer -funwind-tables
cflags_c = 
cflags_cc = -fno-exceptions -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wsign-compare -Wnon-virtual-dtor $
    -Woverloaded-virtual -Wnon-virtual-dtor -Woverloaded-virtual -Wno-abi $
    -std=gnu++11 -Wno-narrowing -Wno-literal-suffix

build obj/webrtc/modules/audio_device/audio_device.audio_device_buffer.o: $
    cxx ../../../../webrtc/modules/audio_device/audio_device_buffer.cc
build obj/webrtc/modules/audio_device/audio_device.audio_device_generic.o: $
    cxx ../../../../webrtc/modules/audio_device/audio_device_generic.cc
build obj/webrtc/modules/audio_device/audio_device.audio_device_utility.o: $
    cxx ../../../../webrtc/modules/audio_device/audio_device_utility.cc
build obj/webrtc/modules/audio_device/audio_device.audio_device_impl.o: cxx $
    ../../../../webrtc/modules/audio_device/audio_device_impl.cc
build $
    obj/webrtc/modules/audio_device/dummy/audio_device.audio_device_dummy.o: $
    cxx ../../../../webrtc/modules/audio_device/dummy/audio_device_dummy.cc
build $
    obj/webrtc/modules/audio_device/dummy/audio_device.audio_device_utility_dummy.o: $
    cxx $
    ../../../../webrtc/modules/audio_device/dummy/audio_device_utility_dummy.cc
build $
    obj/webrtc/modules/audio_device/dummy/audio_device.file_audio_device.o: $
    cxx ../../../../webrtc/modules/audio_device/dummy/file_audio_device.cc
build $
    obj/webrtc/modules/audio_device/dummy/audio_device.file_audio_device_factory.o: $
    cxx $
    ../../../../webrtc/modules/audio_device/dummy/file_audio_device_factory.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.audio_device_utility_android.o: $
    cxx $
    ../../../../webrtc/modules/audio_device/android/audio_device_utility_android.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.audio_manager_jni.o: $
    cxx ../../../../webrtc/modules/audio_device/android/audio_manager_jni.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.audio_record_jni.o: $
    cxx ../../../../webrtc/modules/audio_device/android/audio_record_jni.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.audio_track_jni.o: $
    cxx ../../../../webrtc/modules/audio_device/android/audio_track_jni.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.fine_audio_buffer.o: $
    cxx ../../../../webrtc/modules/audio_device/android/fine_audio_buffer.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.low_latency_event_posix.o: $
    cxx $
    ../../../../webrtc/modules/audio_device/android/low_latency_event_posix.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_common.o: $
    cxx ../../../../webrtc/modules/audio_device/android/opensles_common.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_input.o: $
    cxx ../../../../webrtc/modules/audio_device/android/opensles_input.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_output.o: $
    cxx ../../../../webrtc/modules/audio_device/android/opensles_output.cc
build $
    obj/webrtc/modules/audio_device/android/audio_device.single_rw_fifo.o: $
    cxx ../../../../webrtc/modules/audio_device/android/single_rw_fifo.cc

build obj/webrtc/modules/libaudio_device.a: alink $
    obj/webrtc/modules/audio_device/audio_device.audio_device_buffer.o $
    obj/webrtc/modules/audio_device/audio_device.audio_device_generic.o $
    obj/webrtc/modules/audio_device/audio_device.audio_device_utility.o $
    obj/webrtc/modules/audio_device/audio_device.audio_device_impl.o $
    obj/webrtc/modules/audio_device/dummy/audio_device.audio_device_dummy.o $
    obj/webrtc/modules/audio_device/dummy/audio_device.audio_device_utility_dummy.o $
    obj/webrtc/modules/audio_device/dummy/audio_device.file_audio_device.o $
    obj/webrtc/modules/audio_device/dummy/audio_device.file_audio_device_factory.o $
    obj/webrtc/modules/audio_device/android/audio_device.audio_device_utility_android.o $
    obj/webrtc/modules/audio_device/android/audio_device.audio_manager_jni.o $
    obj/webrtc/modules/audio_device/android/audio_device.audio_record_jni.o $
    obj/webrtc/modules/audio_device/android/audio_device.audio_track_jni.o $
    obj/webrtc/modules/audio_device/android/audio_device.fine_audio_buffer.o $
    obj/webrtc/modules/audio_device/android/audio_device.low_latency_event_posix.o $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_common.o $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_input.o $
    obj/webrtc/modules/audio_device/android/audio_device.opensles_output.o $
    obj/webrtc/modules/audio_device/android/audio_device.single_rw_fifo.o
