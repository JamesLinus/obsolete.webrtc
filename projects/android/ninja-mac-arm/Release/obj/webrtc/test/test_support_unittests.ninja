build obj/webrtc/test/test_support_unittests.actions_depends.stamp: stamp $
    obj/webrtc/test/libchannel_transport.a $
    obj/webrtc/test/libtest_support_main.a $
    obj/webrtc/system_wrappers/source/libsystem_wrappers.a $
    obj/webrtc/base/librtc_base_approved.a $
    obj/webrtc/system_wrappers/source/libcpu_features_android.a $
    obj/third_party/android_tools/ndk/libcpu_features.a $
    obj/webrtc/test/libfield_trial.a obj/webrtc/test/libtest_support.a $
    obj/third_party/gflags/libgflags.a $
    obj/webrtc/system_wrappers/source/libmetrics_default.a

defines = -DV8_DEPRECATION_WARNINGS -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC $
    -DDISABLE_NACL -DCHROMIUM_BUILD -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 $
    -DUSE_PROPRIETARY_CODECS -DENABLE_BROWSER_CDMS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_NOTIFICATIONS $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DDONT_EMBED_BUILD_METADATA $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_BASIC_PRINTING=1 -DENABLE_SUPERVISED_USERS=1 -DVIDEO_HOLE=1 $
    -DV8_USE_EXTERNAL_STARTUP_DATA -DWEBRTC_RESTRICT_LOGGING $
    -DEXPAT_RELATIVE_PATH -DWEBRTC_MODULE_UTILITY_VIDEO -DWEBRTC_ARCH_ARM $
    -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON -DWEBRTC_POSIX $
    -DWEBRTC_LINUX -DWEBRTC_ANDROID -DWEBRTC_ANDROID_OPENSLES $
    -DUSE_LIBPCI=1 -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DANDROID $
    -D__GNU_SOURCE=1 -D_STLP_USE_PTR_SPECIALIZATIONS=1 $
    '-DCHROME_BUILD_ID=""' -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -Igen -I../../../..
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-unused-local-typedefs -Wextra -Wno-unused-parameter $
    -Wno-missing-field-initializers -Wno-strict-overflow -fno-builtin-cos $
    -fno-builtin-sin -fno-builtin-cosf -fno-builtin-sinf -march=armv7-a $
    -mtune=generic-armv7-a -mfpu=vfpv3-d16 -mfloat-abi=softfp -mthumb $
    -fno-tree-sra -fno-caller-saves -Wno-psabi -mthumb-interwork $
    -ffunction-sections -funwind-tables -g -fstack-protector $
    -fno-short-enums -finline-limit=64 -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -isystem$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.7/include $
    -isystem$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.7/libs/armeabi-v7a/include $
    -Os -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-exceptions -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wsign-compare -Wnon-virtual-dtor $
    -Woverloaded-virtual -Wno-abi -std=gnu++11 -Wno-narrowing $
    -Wno-literal-suffix

build $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_transport_unittest.o: $
    cxx ../../../../webrtc/test/channel_transport/udp_transport_unittest.cc
build $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_socket_manager_unittest.o: $
    cxx $
    ../../../../webrtc/test/channel_transport/udp_socket_manager_unittest.cc
build $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_socket_wrapper_unittest.o: $
    cxx $
    ../../../../webrtc/test/channel_transport/udp_socket_wrapper_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.always_passing_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/always_passing_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.fileutils_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/fileutils_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.frame_reader_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/frame_reader_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.frame_writer_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/frame_writer_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.packet_reader_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/packet_reader_unittest.cc
build $
    obj/webrtc/test/testsupport/test_support_unittests.perf_test_unittest.o: $
    cxx ../../../../webrtc/test/testsupport/perf_test_unittest.cc

ldflags = -Wl,-z,now -Wl,-z,relro -Wl,--fatal-warnings -Wl,-z,noexecstack $
    -fPIC -fuse-ld=gold -Wl,--no-undefined $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -nostdlib -Wl,--icf=safe $
    -L$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.7/libs/armeabi-v7a $
    -Wl,-shared,-Bsymbolic $
    $android_ndk_path/platforms/android-14/arch-arm/usr/lib/crtbegin_so.o $
    -Wl,-O1 -Wl,--as-needed -Wl,--gc-sections -Wl,--warn-shared-textrel
libs =  -llog -lgnustl_static $
    $android_ndk_path/toolchains/arm-linux-androideabi-4.7/prebuilt/darwin-x86_64/bin/../lib/gcc/arm-linux-androideabi/4.7/libgcc.a $
    -lc -ldl -lm $
    $android_ndk_path/platforms/android-14/arch-arm/usr/lib/crtend_so.o
build lib/libtest_support_unittests.so $
    lib/libtest_support_unittests.so.TOC: solink $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_transport_unittest.o $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_socket_manager_unittest.o $
    obj/webrtc/test/channel_transport/test_support_unittests.udp_socket_wrapper_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.always_passing_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.fileutils_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.frame_reader_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.frame_writer_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.packet_reader_unittest.o $
    obj/webrtc/test/testsupport/test_support_unittests.perf_test_unittest.o $
    obj/webrtc/test/libchannel_transport.a $
    obj/webrtc/test/libtest_support_main.a $
    obj/webrtc/system_wrappers/source/libsystem_wrappers.a $
    obj/webrtc/base/librtc_base_approved.a $
    obj/webrtc/system_wrappers/source/libcpu_features_android.a $
    obj/third_party/android_tools/ndk/libcpu_features.a $
    obj/webrtc/test/libfield_trial.a obj/webrtc/test/libtest_support.a $
    obj/third_party/gflags/libgflags.a $
    obj/webrtc/system_wrappers/source/libmetrics_default.a
  ld = $ldxx
  soname = libtest_support_unittests.so
  lib = lib/libtest_support_unittests.so
  link_file_list = lib/libtest_support_unittests.so.rsp
