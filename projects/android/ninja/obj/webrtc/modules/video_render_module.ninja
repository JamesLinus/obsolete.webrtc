defines = -DV8_DEPRECATION_WARNINGS -DBLINK_SCALE_FILTERS_AT_RECORD_TIME $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DENABLE_EGLIMAGE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_MANAGED_USERS=1 -DWEBRTC_MODULE_UTILITY_VIDEO $
    -DWEBRTC_ARCH_ARM -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON $
    -DWEBRTC_POSIX -DWEBRTC_LINUX -DWEBRTC_ANDROID $
    -DWEBRTC_ANDROID_OPENSLES -DWEBRTC_INCLUDE_INTERNAL_VIDEO_RENDER $
    -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 -D__STDC_CONSTANT_MACROS $
    -D__STDC_FORMAT_MACROS -DANDROID -D__GNU_SOURCE=1 -DUSE_STLPORT=1 $
    -D_STLP_USE_PTR_SPECIALIZATIONS=1 '-DCHROME_BUILD_ID=""' $
    -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0 $
    -D_FORTIFY_SOURCE=2
includes = -I../../.. -I../../.. -I../../../webrtc/common_video/interface $
    -I../../../webrtc/common_video/libyuv/include $
    -I../../../webrtc/system_wrappers/interface
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-strict-overflow -fno-builtin-cos -fno-builtin-sin $
    -fno-builtin-cosf -fno-builtin-sinf -Wextra -Wno-unused-parameter $
    -Wno-missing-field-initializers -Wno-strict-overflow -fno-builtin-cos $
    -fno-builtin-sin -fno-builtin-cosf -fno-builtin-sinf -march=armv7-a $
    -mfpu=vfpv3-d16 -mfloat-abi=softfp -mthumb -fno-tree-sra -fuse-ld=gold $
    -Wno-psabi -mthumb-interwork -ffunction-sections -funwind-tables -g $
    -fstack-protector -fno-short-enums -finline-limit=64 -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -I$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.6/include $
    -I$android_ndk_path/sources/cxx-stl/gnu-libstdc++/4.6/libs/armeabi-v7a/include $
    -Os -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wsign-compare -Wnon-virtual-dtor -Woverloaded-virtual $
    -Wnon-virtual-dtor -Woverloaded-virtual -Wno-abi

build $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_impl.o: $
    cxx $
    ../../../webrtc/modules/video_render/android/video_render_android_impl.cc
build $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_native_opengl2.o: $
    cxx $
    ../../../webrtc/modules/video_render/android/video_render_android_native_opengl2.cc
build $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_surface_view.o: $
    cxx $
    ../../../webrtc/modules/video_render/android/video_render_android_surface_view.cc
build $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_opengles20.o: $
    cxx $
    ../../../webrtc/modules/video_render/android/video_render_opengles20.cc
build $
    obj/webrtc/modules/video_render/external/video_render_module.video_render_external_impl.o: $
    cxx $
    ../../../webrtc/modules/video_render/external/video_render_external_impl.cc
build $
    obj/webrtc/modules/video_render/video_render_module.incoming_video_stream.o: $
    cxx ../../../webrtc/modules/video_render/incoming_video_stream.cc
build $
    obj/webrtc/modules/video_render/video_render_module.video_render_frames.o: $
    cxx ../../../webrtc/modules/video_render/video_render_frames.cc
build $
    obj/webrtc/modules/video_render/video_render_module.video_render_impl.o: $
    cxx ../../../webrtc/modules/video_render/video_render_impl.cc

build obj/webrtc/modules/libvideo_render_module.a: alink_thin $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_impl.o $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_native_opengl2.o $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_android_surface_view.o $
    obj/webrtc/modules/video_render/android/video_render_module.video_render_opengles20.o $
    obj/webrtc/modules/video_render/external/video_render_module.video_render_external_impl.o $
    obj/webrtc/modules/video_render/video_render_module.incoming_video_stream.o $
    obj/webrtc/modules/video_render/video_render_module.video_render_frames.o $
    obj/webrtc/modules/video_render/video_render_module.video_render_impl.o
