defines = -DV8_DEPRECATION_WARNINGS -DBLINK_SCALE_FILTERS_AT_RECORD_TIME $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DENABLE_EGLIMAGE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_MANAGED_USERS=1 -DWEBRTC_MODULE_UTILITY_VIDEO $
    -DWEBRTC_ARCH_ARM -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON $
    -DWEBRTC_POSIX -DWEBRTC_LINUX -DWEBRTC_ANDROID $
    -DWEBRTC_ANDROID_OPENSLES -DU_USING_ICU_NAMESPACE=0 $
    -DU_STATIC_IMPLEMENTATION -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DANDROID $
    -D__GNU_SOURCE=1 -D_STLP_USE_PTR_SPECIALIZATIONS=1 $
    '-DCHROME_BUILD_ID=""' -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../../.. -I../../.. -I../../../webrtc/common_video/interface $
    -I../../../webrtc/common_video/libyuv/include $
    -I../../../webrtc/system_wrappers/interface $
    -I../../../third_party/icu/source/common $
    -I../../../third_party/jsoncpp/overrides/include $
    -I../../../third_party/jsoncpp/source/include
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-strict-overflow -fno-builtin-cos -fno-builtin-sin $
    -fno-builtin-cosf -fno-builtin-sinf -Wextra -Wno-unused-parameter $
    -Wno-missing-field-initializers -Wno-strict-overflow -fno-builtin-cos $
    -fno-builtin-sin -fno-builtin-cosf -fno-builtin-sinf -target arm-linux-androideabi -mcpu=cortex-a9 $
     -ffunction-sections -funwind-tables -g $
    -fstack-protector -fno-short-enums  -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -I$android_ndk_path/sources/cxx-stl/llvm-libc++/libcxx/include $
    -I$android_ndk_path/sources/android/support/include -U__APPLE__ -D__ANDROID__ -D__linux__ $
    -Os -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wsign-compare -Wnon-virtual-dtor -Woverloaded-virtual $
    -Wnon-virtual-dtor -Woverloaded-virtual -Wno-abi

build $
    obj/webrtc/modules/video_capture/video_capture_module.device_info_impl.o: $
    cxx ../../../webrtc/modules/video_capture/device_info_impl.cc
build $
    obj/webrtc/modules/video_capture/video_capture_module.video_capture_factory.o: $
    cxx ../../../webrtc/modules/video_capture/video_capture_factory.cc
build $
    obj/webrtc/modules/video_capture/video_capture_module.video_capture_impl.o: $
    cxx ../../../webrtc/modules/video_capture/video_capture_impl.cc
build $
    obj/webrtc/modules/video_capture/android/video_capture_module.device_info_android.o: $
    cxx ../../../webrtc/modules/video_capture/android/device_info_android.cc
build $
    obj/webrtc/modules/video_capture/android/video_capture_module.video_capture_android.o: $
    cxx ../../../webrtc/modules/video_capture/android/video_capture_android.cc

build obj/webrtc/modules/libvideo_capture_module.a: alink_thin $
    obj/webrtc/modules/video_capture/video_capture_module.device_info_impl.o $
    obj/webrtc/modules/video_capture/video_capture_module.video_capture_factory.o $
    obj/webrtc/modules/video_capture/video_capture_module.video_capture_impl.o $
    obj/webrtc/modules/video_capture/android/video_capture_module.device_info_android.o $
    obj/webrtc/modules/video_capture/android/video_capture_module.video_capture_android.o
