defines = -DV8_DEPRECATION_WARNINGS -DBLINK_SCALE_FILTERS_AT_RECORD_TIME $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DENABLE_EGLIMAGE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_MANAGED_USERS=1 -DWEBRTC_MODULE_UTILITY_VIDEO $
    -DWEBRTC_ARCH_ARM -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON $
    -DWEBRTC_POSIX -DWEBRTC_LINUX -DWEBRTC_ANDROID $
    -DWEBRTC_ANDROID_OPENSLES -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DANDROID $
    -D__GNU_SOURCE=1 -D_STLP_USE_PTR_SPECIALIZATIONS=1 $
    '-DCHROME_BUILD_ID=""' -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../../.. -I../../.. $
    -I../../../webrtc/common_audio/resampler/include $
    -I../../../webrtc/common_audio/signal_processing/include $
    -I../../../webrtc/common_audio/vad/include $
    -I../../../webrtc/common_video/interface $
    -I../../../webrtc/common_video/libyuv/include $
    -I../../../webrtc/system_wrappers/interface
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-strict-overflow -fno-builtin-cos -fno-builtin-sin $
    -fno-builtin-cosf -fno-builtin-sinf -Wextra -Wno-unused-parameter $
    -Wno-missing-field-initializers -Wno-strict-overflow -fno-builtin-cos $
    -fno-builtin-sin -fno-builtin-cosf -fno-builtin-sinf -target arm-linux-androideabi -mcpu=cortex-a9 $
     -ffunction-sections -funwind-tables -g $
    -fstack-protector -fno-short-enums  -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -I$android_ndk_path/sources/cxx-stl/llvm-libc++/libcxx/include $
    -I$android_ndk_path/sources/android/support/include -U__APPLE__ -D__ANDROID__ -D__linux__ $
    -Os -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wsign-compare -Wnon-virtual-dtor -Woverloaded-virtual $
    -Wnon-virtual-dtor -Woverloaded-virtual -Wno-abi

build $
    obj/webrtc/modules/video_processing/main/source/video_processing.brighten.o: $
    cxx ../../../webrtc/modules/video_processing/main/source/brighten.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.brightness_detection.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/brightness_detection.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.color_enhancement.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/color_enhancement.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.content_analysis.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/content_analysis.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.deflickering.o: $
    cxx ../../../webrtc/modules/video_processing/main/source/deflickering.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.denoising.o: $
    cxx ../../../webrtc/modules/video_processing/main/source/denoising.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.frame_preprocessor.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/frame_preprocessor.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.spatial_resampler.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/spatial_resampler.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.video_decimator.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/video_decimator.cc
build $
    obj/webrtc/modules/video_processing/main/source/video_processing.video_processing_impl.o: $
    cxx $
    ../../../webrtc/modules/video_processing/main/source/video_processing_impl.cc

build obj/webrtc/modules/libvideo_processing.a: alink_thin $
    obj/webrtc/modules/video_processing/main/source/video_processing.brighten.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.brightness_detection.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.color_enhancement.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.content_analysis.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.deflickering.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.denoising.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.frame_preprocessor.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.spatial_resampler.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.video_decimator.o $
    obj/webrtc/modules/video_processing/main/source/video_processing.video_processing_impl.o
