defines = -DV8_DEPRECATION_WARNINGS -DBLINK_SCALE_FILTERS_AT_RECORD_TIME $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DENABLE_EGLIMAGE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_MANAGED_USERS=1 -DWEBRTC_MODULE_UTILITY_VIDEO $
    -DWEBRTC_ARCH_ARM -DWEBRTC_ARCH_ARM_V7 -DWEBRTC_DETECT_ARM_NEON $
    -DWEBRTC_POSIX -DWEBRTC_LINUX -DWEBRTC_ANDROID $
    -DWEBRTC_ANDROID_OPENSLES -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS -DANDROID $
    -D__GNU_SOURCE=1 -D_STLP_USE_PTR_SPECIALIZATIONS=1 $
    '-DCHROME_BUILD_ID=""' -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../../.. -I../../..
cflags = -fstack-protector --param=ssp-buffer-size=4 -Werror $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC -Wextra $
    -Wno-unused-parameter -Wno-missing-field-initializers $
    -Wno-strict-overflow -fno-builtin-cos -fno-builtin-sin $
    -fno-builtin-cosf -fno-builtin-sinf -mfpu=neon -flax-vector-conversions $
    -march=armv7-a -mfloat-abi=softfp -mthumb -fno-tree-sra -fuse-ld=gold $
    -Wno-psabi -mthumb-interwork -ffunction-sections -funwind-tables -g $
    -fstack-protector -fno-short-enums -finline-limit=64 -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -I$android_ndk_path/sources/cxx-stl/gnu-libstdc++/$toolchain_version/include $
    -I$android_ndk_path/sources/cxx-stl/gnu-libstdc++/$toolchain_version/libs/armeabi-v7a/include $
    -Os -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wsign-compare -Wnon-virtual-dtor -Woverloaded-virtual -Wno-abi

build obj/webrtc/common_audio/common_audio_neon.fir_filter_neon.o: cxx $
    ../../../webrtc/common_audio/fir_filter_neon.cc
build $
    obj/webrtc/common_audio/resampler/common_audio_neon.sinc_resampler_neon.o: $
    cxx ../../../webrtc/common_audio/resampler/sinc_resampler_neon.cc
build $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.cross_correlation_neon.o: $
    cc ../../../webrtc/common_audio/signal_processing/cross_correlation_neon.S
build $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.downsample_fast_neon.o: $
    cc ../../../webrtc/common_audio/signal_processing/downsample_fast_neon.S
build $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.min_max_operations_neon.o: $
    cc $
    ../../../webrtc/common_audio/signal_processing/min_max_operations_neon.S
build $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.vector_scaling_operations_neon.o: $
    cc $
    ../../../webrtc/common_audio/signal_processing/vector_scaling_operations_neon.S

build obj/webrtc/common_audio/libcommon_audio_neon.a: alink_thin $
    obj/webrtc/common_audio/common_audio_neon.fir_filter_neon.o $
    obj/webrtc/common_audio/resampler/common_audio_neon.sinc_resampler_neon.o $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.cross_correlation_neon.o $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.downsample_fast_neon.o $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.min_max_operations_neon.o $
    obj/webrtc/common_audio/signal_processing/common_audio_neon.vector_scaling_operations_neon.o
