build obj/third_party/libvpx/gen_asm_offsets_vp8.actions_depends.stamp: $
    stamp obj/third_party/libvpx/libvpx_asm_offsets_vp8.a $
    libvpx_obj_int_extract

rule $
    gen_asm_offsets_vp8_target_unpack_lib_posix_a4c5c66c6ba5295a42df95a5bfd35d10
  command = cd ../../../third_party/libvpx; $
      ../../third_party/libvpx/unpack_lib_posix.sh -d $
      ../../projects/android/ninja/obj/third_party/libvpx/gen_asm_offsets_vp8.gen $
      -f vp8_asm_enc_offsets.o -a $
      ../../projects/android/ninja/libvpx_asm_offsets_vp8.a -a $
      ../../projects/android/ninja/obj/third_party/libvpx/libvpx_asm_offsets_vp8.a $
      -a $
      ../../projects/android/ninja/obj/Source/WebKit/chromium/third_party/libvpx/libvpx_asm_offsets_vp8.a $
      -a ""
  description = ACTION gen_asm_offsets_vp8: $
      unpack_lib_posix_a4c5c66c6ba5295a42df95a5bfd35d10
  restat = 1

build obj/third_party/libvpx/gen_asm_offsets_vp8.gen/vp8_asm_enc_offsets.o: $
    gen_asm_offsets_vp8_target_unpack_lib_posix_a4c5c66c6ba5295a42df95a5bfd35d10 $
    ../../../third_party/libvpx/unpack_lib_posix.sh || $
    obj/third_party/libvpx/gen_asm_offsets_vp8.actions_depends.stamp

rule $
    gen_asm_offsets_vp8_target_obj_int_extract_a4c5c66c6ba5295a42df95a5bfd35d10
  command = cd ../../../third_party/libvpx; python $
      ../../third_party/libvpx/obj_int_extract.py -e $
      ../../projects/android/ninja/libvpx_obj_int_extract -f gas -b $
      "${source}" -o $
      "../../projects/android/ninja/gen/third_party/libvpx/${root}.asm"
  description = RULE Generate assembly offsets ${source}
  restat = 1

build gen/third_party/libvpx/vp8_asm_enc_offsets.asm: $
    gen_asm_offsets_vp8_target_obj_int_extract_a4c5c66c6ba5295a42df95a5bfd35d10 $
    obj/third_party/libvpx/gen_asm_offsets_vp8.gen/vp8_asm_enc_offsets.o | $
    libvpx_obj_int_extract ../../../third_party/libvpx/obj_int_extract.py $
    || obj/third_party/libvpx/gen_asm_offsets_vp8.actions_depends.stamp
  source = $
      ../../projects/android/ninja/obj/third_party/libvpx/gen_asm_offsets_vp8.gen/vp8_asm_enc_offsets.o
  root = vp8_asm_enc_offsets
build $
    obj/third_party/libvpx/gen_asm_offsets_vp8.actions_rules_copies.stamp: $
    stamp $
    obj/third_party/libvpx/gen_asm_offsets_vp8.gen/vp8_asm_enc_offsets.o $
    gen/third_party/libvpx/vp8_asm_enc_offsets.asm

defines = -DV8_DEPRECATION_WARNINGS -DBLINK_SCALE_FILTERS_AT_RECORD_TIME $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_LIBJPEG_TURBO=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY $
    -DDISCARDABLE_MEMORY_ALWAYS_SUPPORTED_NATIVELY $
    -DSYSTEM_NATIVELY_SIGNALS_MEMORY_PRESSURE -DENABLE_EGLIMAGE=1 $
    -DENABLE_AUTOFILL_DIALOG=1 -DCLD_VERSION=1 -DENABLE_PRINTING=1 $
    -DENABLE_MANAGED_USERS=1 -DUSE_OPENSSL=1 -DUSE_OPENSSL_CERTS=1 $
    -DANDROID -D__GNU_SOURCE=1 $
    -D_STLP_USE_PTR_SPECIALIZATIONS=1 '-DCHROME_BUILD_ID=""' $
    -DHAVE_SYS_UIO_H -DNDEBUG -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0
includes = 
cflags = -fstack-protector --param=ssp-buffer-size=4 -fno-exceptions $
    -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-format -target arm-linux-androideabi -mcpu=cortex-a9 $
    -ffunction-sections -funwind-tables -g -fstack-protector $
    -fno-short-enums  -Wa,--noexecstack $
    --sysroot=$android_ndk_path/platforms/android-14/arch-arm $
    -I$android_ndk_path/sources/cxx-stl/llvm-libc++/libcxx/include $
    -Os -I$android_ndk_path/sources/android/support/include -U__APPLE__ -D__ANDROID__ -D__linux__ -fno-ident -fdata-sections -ffunction-sections -fomit-frame-pointer $
    -funwind-tables
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wno-deprecated -Wno-abi


